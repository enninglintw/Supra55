<%= form_for [:admin, @record] do |f| %>
  使用者：<%= f.select :member_id, @m_options %> <br />
  使用者單位：<%= f.select :org_id, @o_options %> <br />
  使用者分類：<%= f.select :identity_id, @i_options %> <br />
  開始時間： <%= f.datetime_select :start_at, :ampm => false, :minute_step => 10 %> <br />
  結束時間： <%= f.datetime_select :end_at, :ampm => false, :minute_step => 10 %> <br />

  <div class="form-actions">
    <%= f.submit "Submit", :disable_with => 'Submitting...', :class => "btn btn-primary"%> 
  </div>
<% end %> 