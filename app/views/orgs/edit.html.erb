<div class="col-md-4 col-md-offset-4">
<h1>編輯使用單位</h1>

<hr>
<%= form_for @org do |f| %>
  名稱：<%= f.text_field :name %> <br />
  全名：<%= f.text_field :official_name %> <br />
  地址：<%= f.text_field :address %> <br />
  電話：<%= f.text_field :tel %> <br />
<hr>
  <%= f.submit "Submit", :disable_with => 'Submitting...', :class => "btn btn-primary"%> 
<% end %> 
</div>